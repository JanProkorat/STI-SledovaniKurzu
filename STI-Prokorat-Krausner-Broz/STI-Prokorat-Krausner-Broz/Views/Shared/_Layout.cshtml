<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="refresh" content="3600; URL=http://localhost:5000">
    <title>@ViewData["Title"] - STI_Prokorat_Krausner_Broz</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.bundle.js"></script>
    <script src="~/js/Scripts.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/js/moment.js" type="text/javascript" asp-append-version="true"></script>
    <link href="@Url.Content("~/css/Styles.css")" rel="stylesheet" type="text/css" asp-append-version="true" />
</head>
    <body>
        <section>
            <div class="container body-content">
                <div id="mySidenav" class="sidenav">
                    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
                    @Html.ActionLink("Zobrazit grafy", "Index")
                    @Html.ActionLink("Zobrazit tabulky", "TableView")
                    @Html.ActionLink("Nejvýhodnější nabídky", "BestDealView")
                </div>

                <span style="font-size:30px;cursor:pointer" onclick="openNav()">&#9776;</span>
                @RenderBody()
            </div>
        </section>
        <footer>
            <p>&copy; 2019 - STI_Prokorat_Krausner_Broz</p>
        </footer>
    </body>
</html>
